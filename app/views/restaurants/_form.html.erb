<% @categories = ["Chinese", "Italian", "Japanese", "French", "Belgian", "Other"] %>

<%= simple_form_for [@restaurant] do |f|%>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, collection: Restaurant.categories %>
  <%= f.button :submit %>
<% end %>
